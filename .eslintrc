{
	"env": {
		"browser": true
	},
	"extends": [
		"airbnb"
	],
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"object-curly-newline": ["error", {
			"ImportDeclaration": "never" // I find oneliners more readable
		}],
		"max-len": ["error", 120],
		"no-tabs": 0,
		"react/jsx-indent": ["error", "tab"],
		"react/jsx-indent-props": ["error", "tab"]
	}
}
